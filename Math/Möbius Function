int mu[N];
bool prime[N];
void build() {
    memset(prime, true, sizeof prime);
    //check 1 or -1
    memset(mu, -1, sizeof mu);
    for(int i = 2;i < N;i++) {
        if(prime[i]) {
            for(int j = i + i;j < N;j += i) {
                prime[j] = false;
                if(j % (1ll * i * i) == 0)
                    mu[j] = 0;
                mu[j] *= -1;
            }
        }
    }
}
