// the number of integrs between 1 to n which are coprime to n
// phi(a * b) = phi(a) * phi(b) * (gcd(a , b) / phi(gcd(a , b))
ll phi(ll x)
{
	ll e = 1;
	for(int i = 2;i <= sqrt(x);i++)
		if(x % i == 0)
		{
			e *= (i - 1);
			x /= i;
			while(x % i == 0)
				x /= i , e *= i;
		}
	if(x > 1)
		e *= (x - 1);
	return e;
}
