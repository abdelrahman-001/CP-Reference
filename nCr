int fact[N] , inv[N];
int fp(int x , int y) {
   if(!y)
       return 1;
   int res = fp(x , y / 2);
   res = (1ll * res * res) % mod;
   if(y & 1)
       res = (1ll * res * x) % mod;
   return res;
}
void pre() {
   fact[0] = inv[0] = 1;
   for(int i = 1;i < N;i++) {
       fact[i] = (1ll * fact[i - 1] * i) % mod;
       inv[i] = fp(fact[i] , mod - 2);
   }
}
int ncr(int n , int r) {
   return (1ll * (1ll * fact[n] * inv[r]) % mod * inv[n - r]) % mod;
}
